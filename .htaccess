RewriteRule ^viewMessages/([^/]*)/([^/]*)/([^/]*)/([^/]*)$ view-messages.php?pId=$1&msgto=$2&msgfrom=$3&mtime=$4 [L]
RewriteRule ^sendTerms terms-sending.php

</IfModule>